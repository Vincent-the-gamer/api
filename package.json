{
  "private": true,
  "scripts": {
    "assets": "tsx prepare/generateMeizi.ts",
    "build": "pnpm run assets && nitro build",
    "dev": "nitro dev --port 8080",
    "lint:fix": "eslint . --fix",
    "prepare": "nitro prepare",
    "start": "NITRO_PORT=3001 node .output/server/index.mjs",
    "dep": "taze major",
    "tsx": "tsx"
  },
  "dependencies": {
    "@wenyan/core": "^0.3.4",
    "fs-extra": "^11.3.0",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "eslint": "^9.22.0",
    "nitropack": "latest",
    "simple-git-hooks": "^2.11.1",
    "taze": "^19.0.2",
    "tsx": "^4.19.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run lint:fix"
  }
